CREATE TABLE BOOK_AUTHOR(
BOK_ID VARCHAR(20,
AUTHOR_NAME VARCHAR2(20)
PRIMARY_KEY(BOOK_ID,AUTHOR_NAME),
FOREIGN KEY(BOOK_TO) REFERENCE BOOK ON DELETE CASCADE);
