CREATE TABLE BOOK(
BOOK_ID VARCHAR2(20) PRIMARY KEY,
TITLE VARCHAR2(40),
PUBLISHER_NAME VARCHAR2(20) REFERENCES
PUBLISHER(NAME) ON DELETE CASCADE,
PUB_YEAR INT);
